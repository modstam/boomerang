{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext"],
      "module": "ESNext",
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
  
      /* Linting */
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noFallthroughCasesInSwitch": true,
      "noUncheckedSideEffectImports": true,
      "noImplicitAny": false,
      "outDir": "dist",
    },
    "include": ["server.ts"]
  }
  